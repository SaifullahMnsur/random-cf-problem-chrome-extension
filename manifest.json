{
  "manifest_version": 3,
  "name": "Random CF Problem",
  "version": "1.1.0",
  "description": "Selects a random problem from Codeforces within a difficulty range",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "https://codeforces.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}